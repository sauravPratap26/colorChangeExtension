{
    "manifest_version": 3,
    "name": "Color Changer",
    "version": "1.0",
    "description": "Changes the background color of the current page",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }